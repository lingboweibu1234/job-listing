




<div class="col-md-8 col-centered">
  <div class="panel panel-info">
    <div class="panel-heading">投交履历到 <%= @job.title %> </div>
    <div class="panel-body">
      <hr>

      <%= simple_form_for [@job, @resume] do |f| %>
        <%= f.input :content %>
        <%= f.input :attachment %>
        <%= f.submit "送出" ,class:'btn btn-danger'%>

      <% end %>
    </div>
  </div>
</div>
